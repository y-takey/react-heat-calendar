{
  "name": "react-heat-calendar",
  "version": "1.1.2",
  "description": "react heat calendar, which is like a Github's contribution calendar",
  "main": "./build/index.js",
  "scripts": {
    "test": "eslint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "NODE_ENV=production webpack",
    "clean": "rimraf build",
    "prepublish": "npm run clean && npm run build",
    "start": "webpack-dev-server --progress --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-takey/react-heat-calendar.git"
  },
  "keywords": [
    "heat-calendar",
    "react",
    "reactjs",
    "react-component"
  ],
  "author": "y-takey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-takey/react-heat-calendar/issues"
  },
  "homepage": "https://github.com/y-takey/react-heat-calendar#readme",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.8.0",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.0.0",
    "jasmine": "~2.3.2",
    "jasmine-core": "~2.3.4",
    "karma": "~0.13.9",
    "karma-jasmine": "~0.3.6",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "rimraf": "^2.3.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash.countby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.0.0"
  }
}
